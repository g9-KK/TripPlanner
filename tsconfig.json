    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "sourceMap": true,
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "types": [
            "node", "mocha"
        ]
    }
}